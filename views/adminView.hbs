                <td>{{this.img_name}}</td>
                <td>{{this.ocrText}}</td>
            </tr>
            {{/each}}


        </tbody>
 </table>
{{#each output}}
    <div class="row">{{this.name}} || {{this.lastName}}</div>
    <div class="row"></div>
    {{/each}}
       
    </div>
    
    



