    <div>
        <table id="table" class="ui table segment">

        <thead>
            <tr>
               
                <th>Patient Name</th>
                <th>Patient ID</th>
                <th>Image Name</th>
                <th>OCR TEXT</th>
            </tr>

        </thead>
    
        <tbody>
            {{#each output}}
